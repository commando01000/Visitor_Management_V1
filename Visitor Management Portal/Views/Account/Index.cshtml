@using Visitor_Management_Portal.Utilities
@{
    ViewBag.Title = "Login";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}

<div class="login-container">
    <!-- Left Section -->
    <div class="left-section">
        <!-- Logo -->

        <div class="logo d-flex flex-column justify-content-center align-items-center gap-3">
            <svg xmlns="http://www.w3.org/2000/svg" width="73.656" height="39.586" viewBox="0 0 73.656 39.586">
                <g id="Group_42915" data-name="Group 42915" transform="translate(-19.53 -38.26)">
