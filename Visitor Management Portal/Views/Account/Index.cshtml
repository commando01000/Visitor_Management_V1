                </g>
            </svg>
            <div class="fw-bold back-btn d-none">  &larr; Back</div>
        </div>

        <!-- Auth Container -->
        <div class="Auth-container">

            <!--Login-form-wrapper-->

            <div class="form-wrapper login-form-wrapper ">
                <h3 class="text-center fw-semiBold fs-3 mb-3">Welcome back</h3>
                <p class="text-center mb-4">Login to your account below</p>


                <button class="singIn-with-microsoft-btn mb-4" onclick="window.location.href='/Account/Signin?provider=@Provider.Microsoft'">
                    <img src="https://img.icons8.com/color/48/microsoft.png" alt="Microsoft Icon" style="width: 20px;" class="me-2">
                    Login With Microsoft
                </button>

                <button class=" signIn-with-google-btn mb-4" onclick="window.location.href='/Account/SignIn?provider=@Provider.Google'">
                    <img src="https://img.icons8.com/color/48/google-logo.png" alt="Google Icon" style="width: 20px;" class="me-2">
                    Login With Google
                </button>

                <div class="d-flex align-items-center my-4">
                    <hr class="flex-grow-1">
                    <span class="mx-1 text-muted">Or</span>
                    <hr class="flex-grow-1">
                </div>

                <form class="login-form">
                    <div class="mb-3">
                        <label for="Login-email" class="form-label">Email Address</label>
                        <input type="email" required class="form-control" id="Login-email" placeholder="Enter Your Email">
                    </div>
                    <div class="mb-3">
                        <label for="password" class="form-label">Password</label>
                        <input type="password" required class="form-control" id="password" placeholder="Enter Your Password">
                    </div>
                    <a href="#" class="d-block mb-3 text-content text-decoration-none forget-password-btn text-end">Forgot your password?</a>
                    <button type="submit" class=" custome-btn login-btn">Login</button>
                </form>

                <p class="text-center mt-4">
                    Don't have an account? <span class="text-content fw-medium go-to-singup-page" role="button">Sign Up</span>
                </p>

                <p class="text-center footer-text mt-5">
                    WayPass 2025. All rights reserved.
                </p>
            </div>

            <!--singUp-form-wrapper-->

            <div class="form-wrapper singUp-form-wrapper d-none">
                <h3 class="text-center  fw-semiBold fs-3 mb-3">Sign Up</h3>
                <p class="text-center mb-4">Enter details to create your new account</p>


                <button class="singIn-with-microsoft-btn mb-4" onclick="window.location.href='/Account/RegisterWithProvider?provider=@Provider.Microsoft'">
                    <img src="https://img.icons8.com/color/48/microsoft.png" alt="Microsoft Icon" style="width: 20px;" class="me-2">
                    Sign Up With Microsoft
                </button>

                <button class=" signIn-with-google-btn mb-4" onclick="window.location.href='/Account/RegisterWithProvider?provider=@Provider.Google'">
                    <img src="https://img.icons8.com/color/48/google-logo.png" alt="Google Icon" style="width: 20px;" class="me-2">
                    Sign Up With Google
                </button>

                <div class="d-flex align-items-center my-4">
                    <hr class="flex-grow-1">
                    <span class="mx-3 text-muted">Or</span>
                    <hr class="flex-grow-1">
                </div>

                <button type="submit" class="  custome-btn singup-btn">Sign Up with Email</button>

                <p class="text-center mt-4">
                    Already have an account? <span class="text-content fw-medium go-to-login-page" role="button">Login</span>
                </p>

                <p class="text-center footer-text mt-5">
                    WayPass 2025. All rights reserved.
                </p>
            </div>

            <!-- Sing Up NavTabs -->

            <div class="form-wrapper singUp-NavTabs d-none ">
                <div class="Register-Step-bar">
                    <div class="bar-1 bar active-bar"></div>
                    <div class="bar-2 bar inactive-bar"></div>
                </div>
                <h3 class="text-center  fw-semiBold fs-3 mb-3">Sign Up</h3>
                <p class="text-center  mb-4">Enter details to create your new account</p>
                @*First Step*@
                <div class="first-singup-step">

                    <div class="mb-3">
                        <label for="fullName" class="form-label">Full Name</label>
                        <input type="text" required class="form-control" id="sign-up-fullName" placeholder="Enter Your Full Name">
                    </div>
                    <div class="mb-3">
                        <label for="email" class="form-label">Email Address</label>
                        <input type="email" required class="form-control" id="sign-up-email" placeholder="Enter Your Email">
                    </div>
                    <div class="mb-3">
                        <label for="password" class="form-label">Password</label>
                        <input type="password" required class="form-control" id="sign-up-password" placeholder="Enter Your Password">
                    </div>

                    <div class="mb-3">
                        <input type="checkbox" id="rememberMe" class="me-2">
                        <label for="rememberMe" class="form-label rememberMe text-content">Remember me</label>
                    </div>
                    <button type="button" class=" custome-btn Continue-btn btn-non-active">Continue</button>

                </div>

                @*Last Step*@
                <div class="last-singup-step d-none">

                    <div class="mb-3">
                        <label for="OrganizationName" class="form-label">Full Name</label>
                        <input type="text" required class="form-control" id="sign-up-OrganizationName" placeholder="Enter The Organization Name">
                    </div>

                    <div class="mb-3">
                        <label for="DomainName" class="form-label">Domain Name</label>
                        <input type="text" required class="form-control" id="sign-up-DomainName" placeholder="Enter The Domain Name">
                    </div>

                    <button type="button" class=" custome-btn Complete-SignUp-btn btn-non-active">Complete Sign Up</button>

                </div>

                <p class="text-center footer-text mt-5">
                    WayPass 2025. All rights reserved.
                </p>
            </div>

            <!-- Reset Password NavTabs -->
            <div class="form-wrapper resetPassword-NavTabs d-none">
                <!-- Step Bar -->
                <div class="Register-Step-bar">
                    <div class="bar bar-1 step-bar step-bar-active"></div>
                    <div class="bar bar-2 step-bar"></div>
                    <div class="bar bar-3 step-bar"></div>
                </div>



                <!-- First Step -->
                <div class="first-resetpassword-step">
                    <h3 class="text-center fw-semiBold fs-3 mb-3">Reset your password</h3>
                    <p class="text-center mb-4">Enter the email address you used to register with</p>
                    <div class="mb-3">
                        <label for="reset-email" class="form-label">Email Address</label>
                        <input required type="email" class="form-control" id="reset-email" placeholder="Enter Your Email">
                    </div>

                    <div class="d-flex flex-column align-items-center gap-3">
                        <button type="button" class="custome-btn btn-non-active Reset-Password-btn-first-step">Reset Password</button>
                        <button type="button" class="custome-btn  Back-to-login-btn">Back to Login</button>
                    </div>
                </div>

                <!-- Second Step -->
                <div class="second-resetpassword-step d-none">
                    <h3 class="text-center fw-semiBold fs-3 mb-3">Password recovery</h3>
                    <p class="text-center mb-4">Check your email for the code and enter it to reset your password</p>

                    <div class="recovery-code-container d-flex justify-content-center gap-2">
                        <input required type="text" maxlength="1" class="code-input">
                        <input required type="text" maxlength="1" class="code-input">
                        <input required type="text" maxlength="1" class="code-input">
                        <input required type="text" maxlength="1" class="code-input">
                    </div>

                    <button type="button" class="custome-btn Continue-btn-resetPassword btn-non-active mt-3">Continue</button>
                    <p class="text-center mt-3">
                        Don't receive the email? <span class="text-content fw-medium resend-email-btn" role="button">Click to resend</span>
                    </p>
                    <button type="button" class="custome-btn Back-to-login-btn">Back to Login</button>
                </div>


                <!-- Last Step -->
                <div class="last-resetpassword-step d-none">
                    <h3 class="text-center fw-semiBold fs-3 mb-3">Enter your new password</h3>
                    <p class="text-center mb-4">Your new password must be different from previous passwords</p>

                    <div class="mb-3">
                        <label for="new-password" class="form-label">New Password</label>
                        <input required type="password" class="form-control" id="new-password" placeholder="Enter New Password">
                    </div>
                    <div class="mb-3">
                        <label for="confirm-password" class="form-label">Confirm Password</label>
                        <input required type="password" class="form-control" id="confirm-password" placeholder="Confirm New Password">
                    </div>

                    <button type="button" class="custome-btn Complete-Reset-Password-btn">Reset Password</button>
                </div>


            </div>

        </div>
    </div>

